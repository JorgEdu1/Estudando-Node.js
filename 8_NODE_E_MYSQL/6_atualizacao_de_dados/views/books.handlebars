<h1>Livros Disponiveis:</h1>
<div class="books-container">
    <a href="/">Adicionar Livro</a>
    {{#each books}}
        <div class="list-books">
            #{{this.id}} - <span class="bold"><a href="/books/{{this.id}}">{{this.title}}</a></span> - {{this.page}} paginas
        </div>
    {{/each}}
</div>